{
  "name": "@mangayomu/nextjs",
  "version": "0.1.0",
  "private": true,
  "description": "The official website for MangaYomu",
  "scripts": {
    "run": "concurrently -k -p \"[{name}]\" -n \"Next.js,Vercel CLI\" -c \"white.bold,cyan.bold\" \"next dev\" \"vercel dev\"",
    "build": "concurrently -p \"[{name}]\" -n \"api,Next.js\" -c \"green.bold,white.bold\" \"yarn run api:build\"",
    "start": "next start",
    "lint": "next lint",
    "reset": "rm -rf node_modules && rm -rf .vercel && rm -rf .next",
    "api:build": "node api-build.js",
    "api:watch": "node api-watch.js",
    "dev": "concurrently -k -p \"[{name}]\" -n \"Vercel CLI,api\" -c \"yellow.bold,green.bold\" \"next dev\" \"yarn run api:watch\""
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@hookform/resolvers": "^3.1.1",
    "@mangayomu/request-handler": "1.0.0",
    "@mangayomu/vercel-env": "1.0.0",
    "@react-spring/web": "^9.7.3",
    "@use-gesture/react": "^10.2.27",
    "axios": "^1.4.0",
    "googleapis": "^118.0.0",
    "http-status-codes": "^2.2.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.0",
    "mdi-react": "^9.2.0",
    "mongoose": "^7.3.1",
    "nanoid": "^4.0.2",
    "next": "13.4.7",
    "react": "18.2.0",
    "react-aria": "^3.26.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.45.1",
    "react-icons": "^4.10.1",
    "react-stately": "^3.24.0",
    "realm-web": "^2.0.0",
    "zod": "^3.21.4",
    "zustand": "^4.3.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.5",
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@emotion/babel-plugin": "^11.11.0",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "20.3.1",
    "@types/react": "18.2.13",
    "@types/react-dom": "18.2.6",
    "autoprefixer": "^10.4.14",
    "babel-plugin-module-resolver": "^5.0.0",
    "concurrently": "^8.2.0",
    "encoding": "^0.1.13",
    "eslint": "8.43.0",
    "eslint-config-next": "13.4.7",
    "postcss": "^8.4.24",
    "tailwindcss": "^3.3.2",
    "typescript": "5.1.3",
    "watch": "^1.0.2"
  }
}
