{
  "name": "@mangayomu/nextjs",
  "version": "0.1.0",
  "private": true,
  "description": "The official website for MangaYomu",
  "scripts": {
    "run": "concurrently -k -p \"[{name}]\" -n \"Next.js,Vercel CLI\" -c \"white.bold,cyan.bold\" \"next dev\" \"vercel dev\"",
    "prebuild": "node init.js && rm -rf ../tsconfig.api.tsbuildinfo",
    "build": "concurrently -p \"[{name}]\" -n \"tsc,Next.js\" -c \"cyan.bold,white.bold\" \"tsc -p tsconfig.api.json && node copy.js\" \"next build\"",
    "postbuild": "rm -rf .tmp",
    "start": "next start",
    "lint": "next lint",
    "reset": "rm -rf node_modules && rm -rf .vercel && rm -rf .next",
    "api:build": "yarn run api:copy",
    "api:copy": "node copy.js",
    "api:watch": "watch \"npm run api:build\" ./.tmp --interval 1 --wait 0.1",
    "dev": "concurrently -k -p \"[{name}]\" -n \"Vercel CLI,api,tsc\" -c \"yellow.bold,green.bold,cyan.bold\" \"next dev\" \"yarn run api:watch\" \"tsc -w -p tsconfig.api.json\" \"node init.js --cleanup\""
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mangayomu/request-handler": "1.0.0",
    "@mangayomu/vercel-env": "1.0.0",
    "axios": "^1.4.0",
    "googleapis": "^118.0.0",
    "http-status-codes": "^2.2.0",
    "next": "13.4.7",
    "next-auth": "^4.22.1",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.11.0",
    "@types/node": "20.3.1",
    "@types/react": "18.2.13",
    "@types/react-dom": "18.2.6",
    "concurrently": "^8.2.0",
    "encoding": "^0.1.13",
    "eslint": "8.43.0",
    "eslint-config-next": "13.4.7",
    "typescript": "5.1.3",
    "watch": "^1.0.2"
  }
}
