{
  "expo": {
    "name": "MangaYomu",
    "slug": "manga-yomu",
    "version": "1.0.0",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "privacy": "unlisted",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "androidStatusBar": {
      "translucent": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.specify_.mangayomu"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
